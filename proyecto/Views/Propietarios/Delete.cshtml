@model proyecto.Models.Propietario

@{
    ViewData["Title"] = "Eliminar Propietario";
}

<h3 style="text-align: center;">ELIMINAR PROPIETARIO N°: @ViewBag.IdPropi</h3>
<hr />
<p style="text-align: center;">¿Estas seguro que quieres eliminarlo? Una vez eliminado no podras recuperar la informacion del propietario.</p>
<div class="contenidoDelete">
    <dl>
        <dt>@Html.DisplayNameFor(model => model.Nombre)</dt>
        <dd>@Html.DisplayFor(model => model.Nombre)</dd>
        <dt>@Html.DisplayNameFor(model => model.Apellido)</dt>
        <dd>@Html.DisplayFor(model => model.Apellido)</dd>
        <dt>@Html.DisplayNameFor(model => model.Dni)</dt>
        <dd>@Html.DisplayFor(model => model.Dni)</dd>
    </dl>
    <dl>
        <dt>@Html.DisplayNameFor(model => model.Telefono)</dt>
        <dd> @Html.DisplayFor(model => model.Telefono)</dd>
        <dt>@Html.DisplayNameFor(model => model.Email)</dt>
        <dd>@Html.DisplayFor(model => model.Email)</dd>
        <div class="form-group" style="margin-top: 10px; float: right;">
            <form asp-action="Delete">
                <input type="button" value="Eliminar" onclick='eliminar(@ViewBag.IdPropi,this,"Propietarios")' class="btn btn-danger" /> |
                <button class="btn btn-secondary" asp-action="Index">Volver atras</button>
            </form>
        </div>
    </dl>
</div>
