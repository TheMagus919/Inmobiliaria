@model proyecto.Models.Contrato

@{
    ViewData["Title"] = "Eliminar Contrato";
}

<h1>Eliminar Contrato</h1>

<h3>Estas seguro de eliminarlo?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Id Contrato
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdContrato)
        </dd>
        <dt class = "col-sm-2">
            Nombre del Propietario del Inmueble
        </dt>
        <dd class = "col-sm-10">
            @(Model.Lugar.Duenio.Nombre + " " + Model.Lugar.Duenio.Apellido)
        </dd>
        <dt class = "col-sm-2">
            <p>Nombre Inquilino</p>
        </dt>
        <dd class = "col-sm-10">
            @(Model.Vive.Nombre + " "+ Model.Vive.Apellido)
        </dd>
        <dt class = "col-sm-2">
            Fecha Desde
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaDesde)
        </dd>
        <dt class = "col-sm-2">
            Fecha Hasta
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaHasta)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <button class="btn btn-secondary" asp-action="Index">Volver atras</button>
    </form>
</div>
